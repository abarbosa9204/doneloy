    public function getDirectivos()
    {
        return $this->directivos;
    }

    /**
     * Add directivo
     *
     * @param string $directivo
     *
     * @return Encuesta
     */
    public function addDirectivo($directivo)
    {
        $directivos = $this->getDirectivos();
        $directivos[] = $directivo;
        $this->setDirectivos($directivos);
        return $this;
    }

    /**
     * Remove directivo
     *
     * @param string $directivo
     *
     * @return Encuesta
     */
    public function removeDirectivo($directivo)
    {
        return $this;
    }

