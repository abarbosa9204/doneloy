     * @param \AppBundle\Entity\User $empresa
     *
     * @return Encuesta
     */
    public function setEmpresa(\AppBundle\Entity\User $empresa = null)
    {
        $this->empresa = $empresa;

        return $this;
    }

    /**
     * Get empresa
     *
     * @return \AppBundle\Entity\User
     */
    public function getEmpresa()
    {
        return $this->empresa;
    }
}
